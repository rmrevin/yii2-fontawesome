{
  "name": "rmrevin/yii2-fontawesome",
  "description": "Asset Bundle for Yii2 with Font Awesome",
  "keywords": [
    "yii",
    "font",
    "awesome",
    "asset",
    "bundle"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "minimum-stability": "stable",
  "support": {
    "issues": "https://github.com/rmrevin/yii2-fontawesome/issues",
    "source": "https://github.com/rmrevin/yii2-fontawesome"
  },
  "authors": [
    {
      "name": "Revin Roman",
      "email": "roman@rmrevin.com",
      "homepage": "https://rmrevin.com/"
    },
    {
      "name": "Simon Karlen",
      "email": "simi.albi@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "fortawesome/font-awesome",
        "version": "5.0.8",
        "source": {
          "url": "https://github.com/FortAwesome/Font-Awesome.git",
          "type": "git",
          "reference": "5.0.8"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.5",
    "yiisoft/yii2": "~2.0",
    "fortawesome/font-awesome": "~5.0"
  },
  "require-dev": {
    "codeception/codeception": "*",
    "yiisoft/yii2-coding-standards": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "rmrevin\\yii\\fontawesome\\": ""
    }
  }
}
